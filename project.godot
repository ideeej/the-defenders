; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "CharacterStats",
"language": "GDScript",
"path": "res://resources/templates/character_stats.gd"
}, {
"base": "Resource",
"class": "CharacterTemplate",
"language": "GDScript",
"path": "res://resources/templates/character_template.gd"
} ]
_global_script_class_icons={
"CharacterStats": "",
"CharacterTemplate": ""
}

[application]

config/name="First Game"
run/main_scene="res://scenes/character_selection.tscn"
config/icon="res://icon.png"

[autoload]

layer_manager="*res://scripts/singletons/layer_manager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://resources/default/default_env.tres"
